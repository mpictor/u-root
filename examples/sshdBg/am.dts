		};

		pinmux_bb_uart2_pins {
			pinctrl-single,pins = <0x180 0x30 0x184 0x00>;
			linux,phandle = <0x51>;
			phandle = <0x51>;
		};

		pinmux_bb_uart3_pins {
			pinctrl-single,pins = <0x150 0x31 0x154 0x01>;
			linux,phandle = <0x52>;
			phandle = <0x52>;
		};

		pinmux_bb_uart5_pins {
			pinctrl-single,pins = <0x168 0x31 0x16c 0x01>;
