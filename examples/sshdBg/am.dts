			#interrupt-cells = <0x01>;
			ti,intc-size = <0x80>;
			reg = <0x48200000 0x1000>;
			linux,phandle = <0x01>;
			phandle = <0x01>;
		};

		edma@49000000 {
			compatible = "ti,edma3";
			ti,hwmods = "tpcc\0tptc0\0tptc1\0tptc2";
