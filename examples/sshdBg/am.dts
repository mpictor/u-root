			linux,phandle = <0x64>;
			phandle = <0x64>;
		};

		pinmux_emmc2_pin {
			pinctrl-single,pins = <0x80 0x32 0x84 0x32 0x00 0x31 0x04 0x31 0x08 0x31 0x0c 0x31 0x10 0x31 0x14 0x31 0x18 0x31 0x1c 0x31>;
			linux,phandle = <0x0d>;
			phandle = <0x0d>;
		};

